  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Demonstrate data type conversion and DataFrame forcing\n",
    "print(\"=== Data Type Conversion ===\")\n",
    "\n",
    "# Create mixed data for demonstration\n",
    "mixed_data = {\n",
    "    'numeric_strings': ['1.5', '2.7', '3.9', 'invalid', '5.1'],\n",
