digraph G {
    rankdir=LR;
    ranksep=0.3;
    nodesep=0.2;
    splines=ortho;
    node [fontname="Helvetica", fontsize=10];
    edge [fontname="Helvetica", fontsize=9];

    python_code [label="Python", shape=box, fillcolor="#1a2634", style=filled, color="#3a4a5a", style=rounded];
    savefig [label="savefig()", shape=box, fillcolor="#1a2634", style=filled, color="#3a4a5a"];
    figz_bundle [label=".figz", shape=box, fillcolor="#0d4a6b", style=filled, color="#5a9fcf"];
    spec_json [label="spec", shape=box, fillcolor="#1a2634", style=filled, color="#3a4a5a"];
    data_csv [label="data", shape=box, fillcolor="#1a2634", style=filled, color="#3a4a5a"];
    preview [label="preview", shape=box, fillcolor="#f0f0f0", style=filled, color="#999999"];
    editor [label="Editor", shape=box, fillcolor="#0d4a6b", style=filled, color="#5a9fcf", style=rounded];
    ai_review [label="AI", shape=diamond, fillcolor="#0d4a6b", style=filled, color="#5a9fcf"];
    export [label="Export", shape=box, fillcolor="#ccffcc", style=filled, color="#00cc00"];

    { rank=same; python_code; savefig; }
    { rank=same; figz_bundle; }
    { rank=same; spec_json; data_csv; preview; }
    { rank=same; editor; ai_review; }
    { rank=same; export; }

    python_code -> savefig;
    savefig -> figz_bundle;
    figz_bundle -> spec_json [style=dashed];
    figz_bundle -> data_csv [style=dashed];
    figz_bundle -> preview [style=dashed];
    figz_bundle -> editor;
    editor -> figz_bundle [style=invis, constraint=true];
    figz_bundle -> ai_review;
    ai_review -> figz_bundle [style=invis, constraint=true];
    figz_bundle -> export;
}