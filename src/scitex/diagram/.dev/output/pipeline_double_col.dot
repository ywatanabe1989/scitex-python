digraph G {
    rankdir=LR;
    ranksep=0.8;
    nodesep=0.5;
    node [fontname="Helvetica", fontsize=10];
    edge [fontname="Helvetica", fontsize=9];

    subgraph cluster_0 {
        label="Processing";
        clean [label="Preprocessing", shape=box, fillcolor="#1a2634", style=filled, color="#3a4a5a"];
        transform [label="Feature Extraction", shape=box, fillcolor="#0d4a6b", style=filled, color="#5a9fcf"];
    }
    subgraph cluster_1 {
        label="Analysis";
        model [label="ML Model", shape=box, fillcolor="#0d4a6b", style=filled, color="#5a9fcf"];
    }
    input [label="Raw Data", shape=box, fillcolor="#1a2634", style=filled, color="#3a4a5a"];
    output [label="Results", shape=box, fillcolor="#1a2634", style=filled, color="#3a4a5a"];


    input -> clean;
    clean -> transform;
    transform -> model;
    model -> output;
}